{
  "name": "typescript-cli",
  "version": "0.0.1",
  "description": "CLI to generate TypeScript Boilerplate",
  "main": "dist/index.js",
  "repository": "git@github.com:AryanshMahato/typescript-cli.git",
  "author": "AryanshMahato <aryanshmahato@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "start:dev": "npm run build && concurrently \"npm run watch\" \"npm run dev\"",
    "start": "node dist/index.js",
    "build": "tsc",
    "build:watch": "onchange 'src/**/*.ts' -- npm run build",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "watch": "tsc -w",
    "dev": "nodemon dist/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.5",
    "@types/source-map-support": "^0.5.3",
    "concurrently": "^5.3.0",
    "jest": "^26.6.1",
    "nodemon": "^2.0.4",
    "onchange": "^7.1.0",
    "prettier": "^2.1.2",
    "serve": "^11.3.2",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.3",
    "typescript": "^4.0.5"
  },
  "keywords": [
    "typescript",
    "typescript-cli",
    "typescript-boilerplate",
    "ts-cli",
    "ts-boilerplate"
  ],
  "bugs": {
    "url": "https://github.com/AryanshMahato/typescript-cli/issues"
  },
  "homepage": "https://github.com/AryanshMahato/typescript-cli#readme",
  "dependencies": {
    "@types/redis": "^2.8.28",
    "node-cache": "^5.1.2",
    "tedis": "^0.1.12"
  }
}
